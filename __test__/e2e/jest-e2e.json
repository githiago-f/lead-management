{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "domain/(.*)": "<rootDir>/../../src/domain/$1",
    "data/(.*)": "<rootDir>/../../src/data/$1",
    "utils/(.*)": "<rootDir>/../../src/utils/$1"
  }
}
